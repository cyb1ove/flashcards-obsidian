{
  "name": "zettelcards",
  "version": "0.2.0",
  "description": "Anki integration oriented towards Zettelkasten system",
  "main": "main.js",
  "scripts": {
    "dev": "rollup --config rollup.config.js -w --environment BUILD:dev",
    "build": "rollup --config rollup.config.js --environment BUILD:production",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}' --quiet",
    "lint:fix": "eslint 'src/**/*.{ts,tsx,js,jsx}' --quiet --fix",
    "test": "jest",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{ts.js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "obsidian",
    "anki",
    "flashcards"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/jest": "^29.2.0",
    "@types/node": "^14.17.4",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.7",
    "jest": "^29.2.2",
    "lint-staged": "^15.5.2",
    "obsidian": "https://github.com/obsidianmd/obsidian-api/tarball/master",
    "rollup": "^2.52.2",
    "rollup-plugin-copy": "^3.4.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "@types/showdown": "^1.9.3",
    "showdown": "^1.9.1"
  }
}
